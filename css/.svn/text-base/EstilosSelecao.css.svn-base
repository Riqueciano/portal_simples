body
{
    margin-left: 0px;
    margin-top: 0px;
    margin-right: 0px;
    margin-bottom: 0px;
    font-family:Arial, Helvetica, sans-serif;
    font-size:12px;
    background-image: url(../Imagens/bg_degrade.jpg);
    background-repeat: repeat-x;
    /*background-image: url(../Imagens/bg_page.png);
    background-repeat: repeat-x;
    background-color: #E9EEF2;*/
}
div
{
    display: block;
}
li
{
    line-height: 18px;
}
input, textarea, select
{
    color:#666;/*COR CINZA*/
}
input[type="button"]
{
    color: #333;
    background: white;
    border: 1px solid #CCC;
}
p#ResumoInscricao, p#Titulo1
{
    margin: 0px;
    /*ESPAÇAMENTO ENTRE AS LINHAS DO PARÁGRAFO*/
    line-height: 18px;
}
a:link, a:visited, a:hover, a:active
{
    text-decoration: none;
    color:#666;/*COR CINZA*/
}
a#logosemarh
{
    background-image: url(../Imagens/logo_semarh.gif);
    background-repeat: no-repeat;
    width: 248px;
    height: 55px;
}
p#Listagem
{
    line-height: 21px;
    text-align:justify;
}
.Oculto {
    border: 1px solid #888888;
    font-size: 10px;
    height: 18px;
    color:#666;
    background-color: #f2f2f2;
}
.alt
{
    display: none;
}
.clear
{
    clear: both;
}
.dataLabel
{
    height:30px;
    color:#666;
    font-weight:bold;
}
.dataField
{
    height:21px;
    color:#666;
}
.titulo1
{
    color:#39B1DB;
